{
  "name": "YouTrack Timer Shortcuts",
  "version": "2.0.0",
  "manifest_version": 3,
  "description": "YouTrack Timer Shortcuts",
  "permissions": ["tabs", "storage", "scripting"],
  "host_permissions": ["*://*.myjetbrains.com/youtrack/agiles/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "./icon16.png",
    "32": "./icon32.png",
    "48": "./icon192.png",
    "192": "./icon192.png",
    "512": "./icon512.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      }
    }
  }
}
